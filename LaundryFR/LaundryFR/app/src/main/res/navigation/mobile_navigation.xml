    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.laundry.fr.ui.PengeluaranFragment"
        android:label="@string/title_pengeluaran"
        tools:layout="@layout/fragment_pengeluaran" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.laundry.fr.ui.PendapatanFragment"
        android:label="@string/title_pendapatan"
        tools:layout="@layout/fragment_pendapatan" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.laundry.fr.ui.TransaksiFragment"
        android:label="@string/title_transaksi"
